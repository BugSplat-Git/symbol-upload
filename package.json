{
  "name": "@bugsplat/symbol-upload",
  "version": "10.2.3",
  "description": "Cross platform symbol upload tool",
  "type": "module",
  "main": "./dist/index.js",
  "exports": {
    ".": "./dist/index.js",
    "./bin": "./dist/bin/index.js"
  },
  "bin": {
    "symbol-upload": "./dist/bin/index.js"
  },
  "engines": {
    "node": ">=22"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "tsx -r dotenv/config ./bin/index.ts -d ./dist -f \"**/*.js.map\"",
    "start:sym": "tsx -r dotenv/config ./bin/index.ts -d ./spec -f \"**/*.sym\"",
    "start:pdb": "tsx -r dotenv/config ./bin/index.ts -d ./spec -f \"**/*.+(exe|dll|pdb)\"",
    "start:dsym": "tsx -r dotenv/config ./bin/index.ts -d ./spec/support -f \"*.dSYM\" -a BugSplatTester -v \"1.0 (1)\"",
    "start:xcarchive": "tsx -r dotenv/config ./bin/index.ts -d ./spec/support -f \"*.xcarchive/**/*.dSYM\" -v \"4.5.6 (1)\"",
    "start:dylib": "tsx -r dotenv/config ./bin/index.ts -d ./spec/support -f \"**/*.dylib.dSYM\"",
    "start:elf": "tsx -r dotenv/config ./bin/index.ts -d ./spec -f \"**/*.elf\"",
    "start:dump_syms": "tsx -r dotenv/config ./bin/index.ts -d ./spec -f \"**/*.dSYM\" -m",
    "start:dump_syms_invalid": "tsx -r dotenv/config ./bin/index.ts -d ./spec -f \"**/myQtCrasher.vc.pdb\" -m",
    "start:symsrv": "tsx -r dotenv/config ./bin/index.ts -d ./spec -f \"**/*.dll\"",
    "start:local": "tsx -r dotenv/config ./bin/index.ts -d ./spec -f \"**/*.sym\" -l ./tmp/local",
    "test": "vitest run",
    "test:watch": "vitest",
    "help": "tsx ./bin/index.ts -h",
    "clean": "rimraf ./dist",
    "prebuild": "npm run clean",
    "build": "npm run build:esm",
    "build:esm": "tsc && npm run copy:compression",
    "copy:compression": "copyfiles -u 1 src/compression.mjs dist/",
    "prerelease": "npm run build",
    "release": "npm publish --access public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BugSplat-Git/symbol-upload.git"
  },
  "private": false,
  "keywords": [
    "symbol",
    "upload",
    "crash",
    "reporting",
    "bugsplat"
  ],
  "author": "@bobbyg603",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/BugSplat-Git/symbol-upload/issues"
  },
  "homepage": "https://github.com/BugSplat-Git/symbol-upload#readme",
  "devDependencies": {
    "@mapbox/node-pre-gyp": "^1.0.11",
    "@types/archiver": "^6.0.2",
    "@types/command-line-args": "^5.2.0",
    "@types/command-line-usage": "^5.0.2",
    "@types/firstline": "^2.0.2",
    "@types/glob": "^8.1.0",
    "vitest": "^2.0.0",
    "@types/node": "^20.14.8",
    "@types/promise-retry": "^1.1.4",
    "@types/workerpool": "^6.4.4",
    "@vercel/ncc": "^0.38.1",
    "bpkg": "^0.9.2",
    "copyfiles": "^2.4.1",
    "dotenv": "^10.0.0",
    "esbuild": "^0.25.9",
    "extract-zip": "^2.0.1",
    "npm-run-all": "^4.1.5",
    "postject": "^1.0.0-alpha.6",
    "rimraf": "^5.0.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.4",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@bugsplat/elfy": "^1.0.1",
    "@bugsplat/js-api-client": "^14.0.0",
    "archiver": "^7.0.1",
    "command-line-args": "^6.0.0",
    "command-line-usage": "^7.0.0",
    "filenamify": "^4.3.0",
    "firstline": "^2.0.2",
    "glob": "^10.3.10",
    "macho-uuid": "^1.3.2",
    "pdb-guid": "^1.0.7",
    "pretty-bytes": "^5.6.0",
    "promise-retry": "^2.0.1",
    "rxjs": "^7.8.1",
    "workerpool": "^6.5.1"
  },
  "optionalDependencies": {
    "node-dump-syms": "^3.0.10"
  }
}
