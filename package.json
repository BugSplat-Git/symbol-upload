{
  "name": "@bugsplat/symbol-upload",
  "version": "6.1.1",
  "description": "Cross platform symbol upload tool",
  "main": "./dist/index.js",
  "bin": {
    "symbol-upload": "./dist/bin/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "ts-node -r dotenv/config ./bin/index.ts -d ./ -f \"**/*.js.map\"",
    "test": "ts-node node_modules/jasmine/bin/jasmine",
    "help": "ts-node ./bin/index.ts -h",
    "clean": "rimraf ./dist",
    "prebuild": "npm run clean",
    "build": "tsc",
    "prerelease": "npm run build",
    "release": "npm publish --access public",
    "prepkg": "npm run build",
    "pkg": "npx pkg -C -o ./pkg/symbol-upload -t node18-macos-x64,node18-linux-x64,node18-win-x64 ./dist/bin/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BugSplat-Git/symbol-upload.git"
  },
  "private": false,
  "keywords": [
    "symbol",
    "upload",
    "crash",
    "reporting",
    "bugsplat"
  ],
  "author": "@bobbyg603",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/BugSplat-Git/symbol-upload/issues"
  },
  "homepage": "https://github.com/BugSplat-Git/symbol-upload#readme",
  "devDependencies": {
    "@types/adm-zip": "^0.4.34",
    "@types/command-line-args": "^5.2.0",
    "@types/command-line-usage": "^5.0.2",
    "@types/firstline": "^2.0.2",
    "@types/glob": "^7.1.3",
    "@types/jasmine": "^4.3.1",
    "@types/node": "^18.15.10",
    "dotenv": "^10.0.0",
    "jasmine": "^4.6.0",
    "pkg": "^5.8.1",
    "rimraf": "^5.0.0",
    "ts-node": "^10.2.1",
    "typescript": "^4.3.2"
  },
  "dependencies": {
    "@bugsplat/js-api-client": "^6.0.1",
    "adm-zip": "^0.5.9",
    "command-line-args": "^5.2.0",
    "command-line-usage": "^6.1.1",
    "firstline": "^2.0.2",
    "glob": "^8.0.3",
    "glob-promise": "^5.0.0",
    "rxjs": "^6.6.6"
  }
}
